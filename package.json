{
  "name": "@cknow/tslint-config",
  "version": "0.0.0-semantically-released",
  "description": "TSLint shareable config",
  "keywords": [
    "tslint",
    "tslint-config",
    "tslint-rules",
    "typescript",
    "config",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "validate",
    "code style",
    "standard",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce"
  ],
  "homepage": "https://github.com/cknow/tslint-config",
  "bugs": {
    "url": "https://github.com/cknow/tslint-config/issues",
    "email": "ricardo@clicknow.com.br"
  },
  "license": "MIT",
  "author": {
    "name": "Ricardo Gobbo de Souza",
    "email": "ricardogobbosouza@yahoo.com.br",
    "role": "Developer"
  },
  "files": [
    "index.js",
    "rules"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cknow/tslint-config.git"
  },
  "scripts": {
    "check-coverage": "nyc check-coverage --global 100",
    "commit": "commit",
    "coverage": "nyc --reporter lcov --reporter text ava",
    "lint": "eslint index.js rules/ test/",
    "postcoverage": "npm run check-coverage",
    "precoverage": "rimraf .nyc_output coverage",
    "pretest": "npm run lint",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "test": "npm run coverage",
    "watch": "ava --watch"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@cknow/eslint-config": "^3.7.0",
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@commitlint/prompt-cli": "^7.1.2",
    "ava": "^0.25.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.6.1",
    "eslint-plugin-ava": "^5.1.1",
    "husky": "^1.1.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "peerDependencies": {
    "tslint": ">=5.11.0"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
